rt Contact from './pages/contact/contact.component';\n\n\n\n\nclass App extends React.Component {\n  constructor(){\n    super();\n\n    this.state={\n      currentUser:null\n    };\n  }\n\n  unsubscribeFromAuth = null;\n\n  componentDidMount() {\n    this.unsubscribeFromAuth = auth.onAuthStateChanged(async userAuth => {\n      if(userAuth) {\n        const userRef=await createUserProfileDocument(userAuth);\n        userRef.onSnapshot(snapShot=> {\n          this.setState({\n            currentUser:{\n              id:snapShot.id,\n              ...snapShot.data()\n            }\n          })\n        })\n      } else {\n        this.setState({currentUser:null})\n      }\n      \n        });\n\n\n  }\n\n  componentWillUnmount() {\n    this.unsubscribeFromAuth();\n    \n  }\n\n\n\n\n  render() {\n    return (\n      <div className='body'>\n        <Route to='/'/>\n        <Header/>\n        <Switch>\n          <Route exact path='/' component = {Homepage}/>\n          <Route exact path='/signin' component={SignInAndSignUpPage}/>\n          <Route path='/checkout' exact component={CheckoutPage}/>\n          <Route path='/contact' exact component={Contact}/>\n          \n          <Route path={`/:collectionId`} component={ItemPage}/>\n        </Switch>\n\n      </div>\n\n    )}\n}\n\n\nconst mapStateToProps = createStructuredSelector ({\n  currentUser: selectCurrentUser\n});\n\nconst mapDispatchToProps = dispatch => ({\n  setCurrentUser: user => dispatch(setCurrentUser(user))\n});\n\nexport default connect(\n  mapStateToProps,\n  mapDispatchToProps\n)(App);","/Users/ojas_kaul/Documents/Ojas/Node/clothing/src/reportWebVitals.js",[],"/Users/ojas_kaul/Documents/Ojas/Node/clothing/src/redux/store.js",[],"/Users/ojas_kaul/Documents/Ojas/Node/clothing/src/firebase/firebase.utils.js",[],"/Users/ojas_kaul/Documents/Ojas/Node/clothing/src/redux/root-reducer.js",["152"],"import { combineReducers } from 'redux';\nimport storage from 'redux-persist/lib/storage';\nimport shopReducer