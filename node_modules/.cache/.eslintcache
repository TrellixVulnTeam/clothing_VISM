React.StrictMode>\n  </Provider>,\n  document.getElementById('root')\n);\n\n// If you want to start measuring performance in your app, pass a function\n// to log results (for example: reportWebVitals(console.log))\n// or send to an analytics endpoint. Learn more: https://bit.ly/CRA-vitals\nreportWebVitals();\n","/Users/ojas_kaul/Documents/Ojas/Node/clothing/src/reportWebVitals.js",[],"/Users/ojas_kaul/Documents/Ojas/Node/clothing/src/App.js",["148","149"],"import React from 'react';\nimport './App.css';\n\nimport { Switch, Route, Redirect } from 'react-router-dom';\nimport {auth, createUserProfileDocument} from './firebase/firebase.utils';\nimport { BrowserRouter } from 'react-router-dom';\nimport {connect} from 'react-redux';\nimport { createStructuredSelector } from 'reselect';\n\n\nimport Header from './components/header/header.component';\nimport Homepage from './pages/homepage/homepage.component';\nimport SignInAndSignUpPage from './pages/sign-in-and-sign-up/sign-in-and-sign-up.component';\nimport CheckoutPage from './pages/checkout/checkout.component' ;\nimport {selectCurrentUser} from './redux/user/user.selectors';\nimport setCurrentUser from './redux/user/user.actions';\nimport ItemPage from './pages/itempage/itempage.component';\nimport Contact from './pages/contact/contact.component';\n\n\n\n\nclass App extends React.Component {\n  constructor(){\n    super();\n\n    this.state={\n      currentUser:null\n    };\n  }\n\n  unsubscribeFromAuth = null;\n\n  componentDidMount() {\n    this.unsubscribeFromAuth = auth.onAuthStateChanged(async userAuth => {\n      if(userAuth) {\n        const userRef=await createUserProfileDocument(userAuth);\n        userRef.onSnapshot(snapShot=> {\n          this.setState({\n            currentUser:{\n              id:snapShot.id,\n              ...snapShot.data()\n            }\n          })\n        })\n      } else {\n        this.setState({currentUser:null})\n      }\n      \n        });\n\n        console.log('inside component did mount')\n        console.log(window.location.href)\n\n  }\n\n  componentWillUnmount() {\n    this.unsubscribeFromAuth();\n    \n  }\n\n\n\n\n  render() {\n    return (\n      <div className='body'>\n        {console.log('routing within app to homepage')}\n        {console.log(window.location.href)}\n        <Route to='/'/>\n        {console.log('routed within app')}\n\n        {console.log(window.location.href)}\n\n        <Header/>\n        <Switch>\n          <Route exact path='/' component = {Homepage}/>\n          <Route exact path='/signin' component={SignInAndSignUpPage}/>\n          <Route path='/checkout' exact component={CheckoutPage}/>\n          <Route path='/contact' exact component={Contact}/>\n          \n          <Route path={`/:collectionId`} component={ItemPage}/>\n        </Switch>\n\n      </div>\n\n    )}\n}\n\n\nconst mapStateToProps = createStructuredSelector ({\n  currentUser: selectCurrentUser\n});\n\nconst mapDispatchToProps = dispatch => ({\n  setCurrentUser: user => dispatch(setCurrentUser(user))\n});\n\nexport default connect(\n  mapStateToProps,\n  mapDispatchToProps\n)(App);","/Users/ojas_kaul/Documents/Ojas/Node/clothing/src/redux/store.js",[],"/Users/ojas_kaul/Documents/Ojas/Node/clothing/src/redux/root-reducer.js",["150"],"import { combineReducers } from 'redux';\nimport storage from 'redux-persist/lib/storage';\nimport shopReducer from './shop/shop.reducer';\nimport userReducer from './user/user.reducer';\nimport cartReducer from './cart/cart-reducer';\n\n\n\n\nconst rootReducer = combineReducers({\n  shop:shopReducer,\n  user: userReducer,\n  cart:cartReducer\n\n})\n\nexport default rootReducer;","/Users/ojas_kaul/Documents/Ojas/Node/clothing/src/firebase/firebase.utils.js",[],"/Users/ojas_kaul/Documents/Ojas/Node/clothing/src/redux/user/user.actions.js",[],"/Users/ojas_kaul/Documents/Ojas/Node/clothing/src/redux/user/user.selectors.js",[],"/Users/ojas_kaul/Documents/Ojas/Node/clothing/src/pages/checkout/checkout.component.jsx",[],"/Users/ojas_kaul/Documents/Ojas/Node/clothing/src/pages/sign-in-and-sign-up/sign-in-and-sign-up.component.jsx",[],"/Users/ojas_kaul/Documents/Ojas/Node/clothing/src/components/header/header.component.jsx",["151","152"],"import React from 'react';\nimport './header.styles.scss';\nimport CartDropdown from '../cart-dropdown/cart-dropdown.component';\nimport { createStructuredSelector } from 'reselect';\nimport {selectCartHidden} from '../../redux/cart/cart-selector';\nimport { connect } from 'react-redux';\nimport { toggleCartHidden } from '../../redux/cart/cart-actions';\n\n\n\nimport {Link} from 'react-router-dom'\n\n\nimport CartIcon from '../cart-icon/cart-icon.component'\n\nconst Header = ({hidden, toggleCartHidden}) => (\n    <div className='header'>\n        <Link className='logo-container' to='/'>\n            CLOTHING\n        </Link>\n\n        <div className='options'>\n            <Link className='option' to='/contact'>\n                CONTACT US\n            </Link>\n\n\n            {currentUser ? (\n            <div className='option' onClick={() => auth.signOut()}>\n            SIGN OUT\n            </div>) :(\n            <Link className='option' to='/signin'>\n            SIGN IN\n            </Link>\n            )}\n\n\n            <div onClick={toggleCartHidden}>\n\n            <CartIcon className='cart-icon'/>\n            </div>\n\n\n        </div>\n        \n            {hidden?null:<CartDropdown/>}\n        \n\n    </div>\n\n);\n\nconst mapStatetoProps= createStructuredSelector({\n    hidden:selectCartHidden\n});\n\nconst mapDispatchToProps = dispatch => ({\n    toggleCartHidden: () => dispatch(toggleCartHidden())\n  });\n\nexport default connect(mapStatetoProps, mapDispatchToProps)(Header);","/Users/ojas_kaul/Documents/Ojas/Node/clothing/src/pages/homepage/homepage.component.jsx",[],"/Users/ojas_kaul/Documents/Ojas/Node/clothing/src/pages/itempage/itempage.component.jsx",["153","154"],"import React from 'react';\nimport './itempage.component.styles.scss';\n\n\n\nimport {connect} from 'react-redux';\nimport { createStructuredSelector } from 'reselect';\n\n\nimport {selectTshirt} from '../../redux/shop/shop.selector';\nimport CustomButton from '../../components/custom-button/custom-button.component';\nimport {addItem} from '../../redux/cart/cart-actions';\n\n\n\n\n\nconst Itempage =({tshirt, match, addItem, history})=>{\n    return(\n    <div className='container-div'>\n         <div className='image-holder'>\n             <img src={`${tshirt.imageUrl}`} width='600px' height='auto'></img>\n         </div>\n         <div className='complete-data'>\n        <div className='data'>\n            <div className='product-name'><h2>NAME: {tshirt.title.toUpperCase()}</h2></div>\n            <div className='product-price'><h3> PRICE: ${tshirt.price}</h3></div>\n            <div className='product-price'><h4> COLOUR: {tshirt.color}</h4></div>\n            <div className='product-price'><h4> MATERIAL: {tshirt.material}</h4></div>\n            <div className='product-price'><h4> DESCRIPTION: {tshirt.description}</h4></div>\n        </div>\n        <div className='buttonss'>\n        <CustomButton onClick={()=> addItem(tshirt)} >ADD TO CART</CustomButton>\n        <CustomButton onClick={()=> history.push('/checkout')}>GO TO CHECKOUT</CustomButton>\n        </div>\n        </div>\n    </div>\n)}\n\n\nconst mapStateToProps = (state, ownProps)=>({\n    tshirt: selectTshirt(ownProps.match.params.collectionId)(state)\n});\n\nconst mapDispatchToProps = dispatch => ({\n    addItem: tshirt => dispatch(addItem(tshirt))\n})\n\n\nexport default connect(mapStateToProps,mapDispatchToProps)(Itempage);","/Users/ojas_kaul/Documents/Ojas/Node/clothing/src/pages/contact/contact.component.jsx",[],"/Users/ojas_kaul/Documents/Ojas/Node/clothing/src/redux/user/user.reducer.js",[],"/Users/ojas_kaul/Documents/Ojas/Node/clothing/src/redux/user/user.types.js",[],"/Users/ojas_kaul/Documents/Ojas/Node/clothing/src/redux/shop/shop.reducer.js",[],"/Users/ojas_kaul/Documents/Ojas/Node/clothing/src/redux/cart/cart-reducer.js",["155"],"\nimport CartActionTypes from './cart.types';\nimport {addItemToCart, removeItemFromCart} from './cart.utils';\n\nconst INITIAL_STATE = {\n  hidden: true,\n  cartItems: []\n};\n\nconst cartReducer = (state = INITIAL_STATE, action) => {\n  switch (action.type) {\n    case CartActionTypes.TOGGLE_CART_HIDDEN:\n      return {\n        ...state,\n        hidden: !state.hidden\n      };\n\n    case CartActionTypes.ADD_ITEM:\n      return {\n        ...state,\n        cartItems: addItemToCart(state.cartItems, action.payload)\n      };\n\n      case CartActionTypes.CLEAR_ITEM_FROM_CART:\n        return {\n          ...state,\n          cartItems: state.cartItems.filter(cartItem => cartItem.id!= action.payload.id)\n        } \n\n\n      case CartActionTypes.REMOVE_ITEM:\n        return {\n         ...state,\n         cartItems: removeItemFromCart(state.cartItems, action.payload)\n        }\n    \n    default:\n      return state;\n  }\n};\n\nexport default cartReducer;","/Users/ojas_kaul/Documents/Ojas/Node/clothing/src/redux/cart/cart-selector.js",[],"/Users/ojas_kaul/Documents/Ojas/Node/clothing/src/redux/cart/cart-actions.js",[],"/Users/ojas_kaul/Documents/Ojas/Node/clothing/src/redux/shop/shop.selector.js",[],"/Users/ojas_kaul/Documents/Ojas/Node/clothing/src/components/checkout-item/checkout-item.component.jsx",[],"/Users/ojas_kaul/Documents/Ojas/Node/clothing/src/components/cart-dropdown/cart-dropdown.component.jsx",["156","157","158","159"],"import React from 'react';\nimport './cart-dropdown.styles.scss';\n\nimport CustomButton from '../custom-button/custom-button.component';\nimport {CartItem} from '../cart-item/cart-item.component'\n\nconst CartDropdown=({cartItems, history, dispatch})=>(\n\n    <div className='cart-dropdown'>\n    <div className='cart-items'>\n        {cartItems.length ?(\n        cartItems.map((cartItem) => (<CartItem key={cartItem.id} item={cartItem}/>))\n        ):(\n            <span className='empty-message'>\n                Your Cart is Empty\n            </span>\n        )}\n    </div>\n    <CustomButton onClick={()=>{\n        history.push('/checkout');\n        dispatch(toggleCartHidden())\n        }}>GO TO CHECKOUT</CustomButton>\n</div>\n)\n\nconst mapStateToProps = createStructuredSelector ({\n    cartItems:selectCartItems\n})\n\nexport default connect(mapStateToProps)(CartDropdown);","/Users/ojas_kaul/Documents/Ojas/Node/clothing/src/components/sign-up/sign-up.component.jsx",[],"/Users/ojas_kaul/Documents/Ojas/Node/clothing/src/components/sign-in/sign-in.component.jsx",[],"/Users/ojas_kaul/Documents/Ojas/Node/clothing/src/components/cart-icon/cart-icon.component.jsx",[],"/Users/ojas_kaul/Documents/Ojas/Node/clothing/src/components/custom-button/custom-button.component.jsx",[],"/Users/ojas_kaul/Documents/Ojas/Node/clothing/src/components/item-card/item-card.component.jsx",["160"],"import React from 'react';\nimport './item-card.styles.scss';\nimport {withRouter} from 'react-router-dom';\n\nimport CustomButton from '../custom-button/custom-button.component';\n\nconst ItemCard=({item,history,match })=>(\n    <div className='item-card'\n    onClick={()=> history.push(`${match.url}${item.title}`)} >\n        \n\n        <div className='image'\n        style={{backgroundImage:`url(${item.imageUrl})`}}/>\n        \n       \n\n        <div className='footer'>\n            <div className='product-name'>{item.title.toUpperCase()}</div>\n            <div className='product-price'>${item.price}</div>\n        </div>\n\n    </div>\n)\n\nexport default withRouter(ItemCard);","/Users/ojas_kaul/Documents/Ojas/Node/clothing/src/components/bootstrap/slider/slider.component.jsx",[],"/Users/ojas_kaul/Documents/Ojas/Node/clothing/src/redux/cart/cart.types.js",[],"/Users/ojas_kaul/Documents/Ojas/Node/clothing/src/redux/shop/shop.data.js",[],"/Users/ojas_kaul/Documents/Ojas/Node/clothing/src/redux/cart/cart.utils.js",["161","162","163"],"\nexport const addItemToCart = (cartItems, cartItemToAdd) => {\n    const existingCartItem = cartItems.find(\n      cartItem => cartItem.id === cartItemToAdd.id\n    );\n  \n    if (existingCartItem) {\n      return cartItems.map(cartItem =>\n        cartItem.id === cartItemToAdd.id\n          ? { ...cartItem, quantity: cartItem.quantity + 1 }\n          : cartItem\n      );\n    }\n  \n    return [...cartItems, { ...cartItemToAdd, quantity: 1 }];\n  };\n\n  export const removeItemFromCart = (cartItems, cartItemToRemove) => {\n    const existingCartItem = cartItems.find(\n      cartItem => cartItem.id===cartItemToRemove.id);\n    \n      if (existingCartItem.quantity==1)\n      {\n        return (cartItems.filter(cartItem=>cartItem.id!=cartItemToRemove.id))\n      }\n\n      else {\n        return cartItems.map(\n          cartItem=> \n          cartItem.id==cartItemToRemove.id\n          ? {...cartItem, quantity:cartItem.quantity-1}\n          : cartItem\n        )\n      }\n  }\n","/Users/ojas_kaul/Documents/Ojas/Node/clothing/src/components/cart-item/cart-item.component.jsx",[],"/Users/ojas_kaul/Documents/Ojas/Node/clothing/src/components/form-input/form-input.component.jsx",[],{"ruleId":"164","severity":1,"message":"165","line":8,"column":16,"nodeType":"166","messageId":"167","endLine":8,"endColumn":25},{"ruleId":"164","severity":1,"message":"168","line":4,"column":25,"nodeType":"166","messageId":"167","endLine":4,"endColumn":33},{"ruleId":"164","severity":1,"message":"169","line":6,"column":10,"nodeType":"166","messageId":"167","endLine":6,"endColumn":23},{"ruleId":"164","severity":1,"message":"170","line":2,"column":8,"nodeType":"166","messageId":"167","endLine":2,"endColumn":15},{"ruleId":"171","severity":2,"message":"172","line":28,"column":14,"nodeType":"166","messageId":"173","endLine":28,"endColumn":25},{"ruleId":"171","severity":2,"message":"174","line":29,"column":52,"nodeType":"166","messageId":"173","endLine":29,"endColumn":56},{"ruleI