edirect } from 'react-router-dom';\r\nimport {auth, createUserProfileDocument} from './firebase/firebase.utils';\r\nimport { BrowserRouter } from 'react-router-dom';\r\nimport {connect} from 'react-redux';\r\nimport { createStructuredSelector } from 'reselect';\r\n\r\n\r\nimport Header from './components/header/header.component';\r\nimport Homepage from './pages/homepage/homepage.component';\r\nimport SignInAndSignUpPage from './pages/sign-in-and-sign-up/sign-in-and-sign-up.component';\r\nimport CheckoutPage from './pages/checkout/checkout.component' ;\r\nimport {selectCurrentUser} from './redux/user/user.selectors';\r\nimport setCurrentUser from './redux/user/user.actions';\r\nimport ItemPage from './pages/itempage/itempage.component';\r\nimport Contact from './pages/contact/contact.component';\r\n\r\n\r\n\r\n\r\nclass App extends React.Component {\r\n  constructor(){\r\n    super();\r\n\r\n    this.state={\r\n      currentUser:null\r\n    };\r\n  }\r\n\r\n  unsubscribeFromAuth = null;\r\n\r\n  componentDidMount() {\r\n    this.unsubscribeFromAuth = auth.onAuthStateChanged(async userAuth => {\r\n      if(userAuth) {\r\n        const userRef=await createUserProfileDocument(userAuth);\r\n        userRef.onSnapshot(snapShot=> {\r\n          this.setState({\r\n            currentUser:{\r\n              id:snapShot.id,\r\n              ...snapShot.data()\r\n            }\r\n          })\r\n        })\r\n      } else {\r\n        this.setState({currentUser:null})\r\n      }\r\n      \r\n        });\r\n\r\n        console.log('inside component did mount')\r\n        console.log(window.location.href)\r\n\r\n  }\r\n\r\n  componentWillUnmount() {\r\n    this.unsubscribeFromAuth();\r\n    \r\n  }\r\n\r\n\r\n\r\n\r\n  render() {\r\n    return (\r\n      <div className='body'>\r\n        {console.log('routing within app to homepage')}\r\n        {console.log(window.location.href)}\r\n        <Route to='/'/>\r\n        {console.log('routed within app')}\r\n\r\n        {console.log(window.location.href)}\r\n\r\n        <Header/>\r\n        <Switch>\r\n          <Route exact path='/' component = {Homepage}/>\r\n          <Route exact path='/signin' component={SignInAndSignUpPage}/>\r\n          <Route path='/checkout' exact component={CheckoutPage}/>\r\n          <Route path='/contact' exact component={Contact}/>\r\n          \r\n          <Route path={`/:collectionId`} component={ItemPage}/>\r\n        </Switch>\r\n\r\n      </div>\r\n\r\n    )}\r\n}\r\n\r\n\r\nconst mapStateToProps = createStructuredSelector ({\r\n  currentUser: selectCurrentUser\r\n});\r\n\r\nconst mapDispatchToProps = dispatch => ({\r\n  setCurrentUser: user => dispatch(setCurrentUser(user))\r\n});\r\n\r\nexport default connect(\r\n  mapStateToProps,\r\n  mapDispatchToProps\r\n)(App);","C:\\Users\\satya\\desktop\\Reactprojects\\clothing\\src\\reportWebVitals.js",[],"C:\\Users\\satya\\desktop\\Reactprojects\\clothing\\src\\redux\\store.js",[],"C:\\Users\\satya\\desktop\\Reactprojects\\clothing\\src\\firebase\\firebase.utils.js",[],"C:\\Users\\satya\\desktop\\Reactprojects\\clothing\\src\\redux\\user\\user.selectors.js",[],"C:\\Users\\satya\\desktop\\Reactprojects\\clothing\\src\\redux\\user\\user.actions.js",[],"C:\\Users\\satya\\desktop\\Reactprojects\\clothing\\src\\redux\\root-reducer.js",["156"],"import { combineReducers } from 'redux';\r\nimport storage from 'redux-persist/lib/storage';\r\nimport shopReducer from './shop/shop.reducer';\r\nimport userReducer from './user/user.reducer';\r\nimport cartReducer from './cart/cart-reducer';\r\n\r\n\r\n\r\n\r\nconst rootReducer = combineReducers({\r\n  shop:shopReducer,\r\n  user: userReducer,\r\n  cart:cartReducer\r\n\r\n})\r\n\r\nexport default rootReducer;","C:\\Users\\satya\\desktop\\Reactprojects\\clothing\\src\\pages\\itempage\\itempage.component.jsx",["157","158"],"import React from 'react';\r\nimport './itempage.component.styles.scss';\r\n\r\n\r\n\r\nimport {connect} from 'react-redux';\r\nimport { createStructuredSelector } from 'reselect';\r\n\r\n\r\nimport {selectTshirt} from '../../redux/shop/shop.selector';\r\nimport CustomButton from '../../components/custom-button/custom-button.component';\r\nimport {addItem} from '../../redux/cart/cart-actions';\r\n\r\n\r\n\r\n\r\n\r\nconst Itempage =({tshirt, match, addItem, history})=>{\r\n    return(\r\n    <div className='container-div'>\r\n         <div className='image-holder'>\r\n             <img src={`${tshirt.imageUrl}`} width='600px' height='auto'></img>\r\n         </div>\r\n         <div className='complete-data'>\r\n        <div className='data'>\r\n            <div className='product-name'><h2>NAME: {tshirt.title.toUpperCase()}</h2></div>\r\n            <div className='product-price'><h3> PRICE: ${tshirt.price}</h3></div>\r\n            <div className='product-price'><h4> COLOUR: {tshirt.color}</h4></div>\r\n            <div className='product-price'><h4> MATERIAL: {tshirt.material}</h4></div>\r\n            <div className='product-price'><h4> DESCRIPTION: {tshirt.description}</h4></div>\r\n        </div>\r\n        <div className='buttonss'>\r\n        <CustomButton onClick={()=> addItem(tshirt)} >ADD TO CART</CustomButton>\r\n        <CustomButton onClick={()=> history.push('/checkout')}>GO TO CHECKOUT</CustomButton>\r\n        </div>\r\n        </div>\r\n    </div>\r\n)}\r\n\r\n\r\nconst mapStateToProps = (state, ownProps)=>({\r\n    tshirt: selectTshirt(ownProps.match.params.collectionId)(state)\r\n});\r\n\r\nconst mapDispatchToProps = dispatch => ({\r\n    addItem: tshirt => dispatch(addItem(tshirt))\r\n})\r\n\r\n\r\nexport default connect(mapStateToProps,mapDispatchToProps)(Itempage);","C:\\Users\\satya\\desktop\\Reactprojects\\clothing\\src\\pages\\sign-in-and-sign-up\\sign-in-and-sign-up.component.jsx",[],"C:\\Users\\satya\\desktop\\Reactprojects\\clothing\\src\\pages\\homepage\\homepage.component.jsx",[],"C:\\Users\\satya\\desktop\\Reactprojects\\clothing\\src\\pages\\checkout\\checkout.component.jsx",[],"C:\\Users\\satya\\desktop\\Reactprojects\\clothing\\src\\pages\\contact\\contact.component.jsx",[],"C:\\Users\\satya\\desktop\\Reactprojects\\clothing\\src\\components\\header\\header.component.jsx",["159"],"import React from 'react';\r\nimport './header.styles.scss';\r\nimport CartDropdown from '../cart-dropdown/cart-dropdown.component';\r\nimport { createStructuredSelector } from 'reselect';\r\nimport {selectCartHidden} from '../../redux/cart/cart-selector';\r\nimport { connect } from 'react-redux';\r\nimport { toggleCart } from '../../redux/cart/cart-actions';\r\n\r\n\r\n\r\nimport {Link} from 'react-router-dom'\r\n\r\n\r\nimport CartIcon from '../cart-icon/cart-icon.component'\r\n\r\nconst Header = ({hidden, toggleCartHidden}) => (\r\n    <div className='header'>\r\n        <Link className='logo-container' to='/'>\r\n            CLOTHING\r\n        </Link>\r\n\r\n        <div className='options'>\r\n            <Link className='option' to='/contact'>\r\n                CONTACT US\r\n            </Link>\r\n\r\n            <Link className='option' to='/signin'>\r\n                SIGN IN\r\n            </Link>\r\n            <div onClick={toggleCartHidden}>\r\n\r\n            <CartIcon className='cart-icon'/>\r\n            </div>\r\n\r\n\r\n        </div>\r\n        \r\n            {hidden?null:<CartDropdown/>}\r\n        \r\n\r\n    </div>\r\n\r\n);\r\n\r\nconst mapStatetoProps= createStructuredSelector({\r\n    hidden:selectCartHidden\r\n});\r\n\r\n\r\n\r\nexport default connect(mapStatetoProps)(Header);","C:\\Users\\satya\\desktop\\Reactprojects\\clothing\\src\\redux\\user\\user.types.js",[],"C:\\Users\\satya\\desktop\\Reactprojects\\clothing\\src\\redux\\user\\user.reducer.js",[],"C:\\Users\\satya\\desktop\\Reactprojects\\clothing\\src\\redux\\shop\\shop.reducer.js",[],"C:\\Users\\satya\\desktop\\Reactprojects\\clothing\\src\\redux\\cart\\cart-reducer.js",["160"],"\r\nimport CartActionTypes from './cart.types';\r\nimport {addItemToCart, removeItemFromCart} from './cart.utils';\r\n\r\nconst INITIAL_STATE = {\r\n  hidden: true,\r\n  cartItems: []\r\n};\r\n\r\nconst cartReducer = (state = INITIAL_STATE, action) => {\r\n  switch (action.type) {\r\n    case CartActionTypes.TOGGLE_CART_HIDDEN:\r\n      return {\r\n        ...state,\r\n        hidden: !state.hidden\r\n      };\r\n\r\n    case CartActionTypes.ADD_ITEM:\r\n      return {\r\n        ...state,\r\n        cartItems: addItemToCart(state.cartItems, action.payload)\r\n      };\r\n\r\n      case CartActionTypes.CLEAR_ITEM_FROM_CART:\r\n        return {\r\n          ...state,\r\n          cartItems: state.cartItems.filter(cartItem => cartItem.id!= action.payload.id)\r\n        } \r\n\r\n\r\n      case CartActionTypes.REMOVE_ITEM:\r\n        return {\r\n         ...state,\r\n         cartItems: removeItemFromCart(state.cartItems, action.payload)\r\n        }\r\n    \r\n    default:\r\n      return state;\r\n  }\r\n};\r\n\r\nexport default cartReducer;","C:\\Users\\satya\\desktop\\Reactprojects\\clothing\\src\\redux\\shop\\shop.selector.js",[],"C:\\Users\\satya\\desktop\\Reactprojects\\clothing\\src\\redux\\cart\\cart-actions.js",[],"C:\\Users\\satya\\desktop\\Reactprojects\\clothing\\src\\redux\\cart\\cart-selector.js",[],"C:\\Users\\satya\\desktop\\Reactprojects\\clothing\\src\\components\\custom-button\\custom-button.component.jsx",[],"C:\\Users\\satya\\desktop\\Reactprojects\\clothing\\src\\components\\sign-in\\sign-in.component.jsx",[],"C:\\Users\\satya\\desktop\\Reactprojects\\clothing\\src\\components\\sign-up\\sign-up.component.jsx",[],"C:\\Users\\satya\\desktop\\Reactprojects\\clothing\\src\\components\\item-card\\item-card.component.jsx",["161"],"import React from 'react';\r\nimport './item-card.styles.scss';\r\nimport {withRouter} from 'react-router-dom';\r\n\r\nimport CustomButton from '../custom-button/custom-button.component';\r\n\r\nconst ItemCard=({item,history,match })=>(\r\n    <div className='item-card'\r\n    onClick={()=> history.push(`${match.url}${item.title}`)} >\r\n        \r\n\r\n        <div className='image'\r\n        style={{backgroundImage:`url(${item.imageUrl})`}}/>\r\n        \r\n       \r\n\r\n        <div className='footer'>\r\n            <div className='product-name'>{item.title.toUpperCase()}</div>\r\n            <div className='product-price'>${item.price}</div>\r\n        </div>\r\n\r\n    </div>\r\n)\r\n\r\nexport default withRouter(ItemCard);","C:\\Users\\satya\\desktop\\Reactprojects\\clothing\\src\\components\\checkout-item\\checkout-item.component.jsx",[],"C:\\Users\\satya\\desktop\\Reactprojects\\clothing\\src\\components\\bootstrap\\slider\\slider.component.jsx",[],"C:\\Users\\satya\\desktop\\Reactprojects\\clothing\\src\\redux\\cart\\cart.types.js",[],"C:\\Users\\satya\\desktop\\Reactprojects\\clothing\\src\\components\\cart-icon\\cart-icon.component.jsx",[],"C:\\Users\\satya\\desktop\\Reactprojects\\clothing\\src\\redux\\shop\\shop.data.js",[],"C:\\Users\\satya\\desktop\\Reactprojects\\clothing\\src\\redux\\cart\\cart.utils.js",["162","163","164"],"\r\nexport const addItemToCart = (cartItems, cartItemToAdd) => {\r\n    const existingCartItem = cartItems.find(\r\n      cartItem => cartItem.id === cartItemToAdd.id\r\n    );\r\n  \r\n    if (existingCartItem) {\r\n      return cartItems.map(cartItem =>\r\n        cartItem.id === cartItemToAdd.id\r\n          ? { ...cartItem, quantity: cartItem.quantity + 1 }\r\n          : cartItem\r\n      );\r\n    }\r\n  \r\n    return [...cartItems, { ...cartItemToAdd, quantity: 1 }];\r\n  };\r\n\r\n  export const removeItemFromCart = (cartItems, cartItemToRemove) => {\r\n    const existingCartItem = cartItems.find(\r\n      cartItem => cartItem.id===cartItemToRemove.id);\r\n    \r\n      if (existingCartItem.quantity==1)\r\n      {\r\n        return (cartItems.filter(cartItem=>cartItem.id!=cartItemToRemove.id))\r\n      }\r\n\r\n      else {\r\n        return cartItems.map(\r\n          cartItem=> \r\n          cartItem.id==cartItemToRemove.id\r\n         